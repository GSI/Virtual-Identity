		<tabs>
			<tab id="idSelection1" label="&vI_prefDialog.idSelection.Tab1.label;" />
			<tab id="idSelection2" label="&vI_prefDialog.idSelection.Tab2.label;" />
		</tabs>
		<tabpanels>
			<tabpanel orient="vertical">
			<groupbox>
			<caption label="&vI_prefDialog.idSelection.Tab1.caption;"/>
			<vbox align="left">
				<description width="500px">&vI_prefDialog.idSelection.reply.desc;</description>
				<checkbox id="VIdent_identity.idSelection_storage_prefer_smart_reply" label="&vI_prefDialog.idSelection.prefer_smart_reply.label;"
					prefstring="idSelection_storage_prefer_smart_reply"/>
				<checkbox id="VIdent_identity.idSelection_storage_ignore_smart_reply" label="&vI_prefDialog.idSelection.ignore_smart_reply.label;"
					prefstring="idSelection_storage_ignore_smart_reply"/>
			</vbox>
			</groupbox>
			<groupbox>
			<caption label="&vI_prefDialog.idSelection.Tab1.caption2;"/>
			<vbox align="left">
				<description width="500px">&vI_prefDialog.idSelection.draft.desc;</description>
			</vbox>
			</groupbox>
			</tabpanel>
			<tabpanel orient="vertical">
			<groupbox>
			<caption label="&vI_prefDialog.idSelection.Tab2.caption;"/>
			<vbox align="left">
				<description width="500px">&vI_prefDialog.idSelection.multiselect.desc;</description>
				<checkbox id="VIdent_identity.idSelection_ask" label="&vI_prefDialog.idSelection.ask.label;"
					prefstring="idSelection_ask"
					oncommand="vI_prefDialog.base.idSelectionResultConstraint();"/>
				<hbox><spacer width="20px"/><checkbox id="VIdent_identity.idSelection_ask_always" label="&vI_prefDialog.idSelection.ask_always.label;"
