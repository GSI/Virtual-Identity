	<tabpanel orient="vertical" label="&vI_prefDialog.notificationTab.header;">
		<dialogheader title="&vI_prefDialog.notificationTab.header;"/>
		<groupbox id="notificationGroupBox">
		<caption label="&vI_prefDialog.notificationTab.caption;"/>
		<vbox align="left">
			<checkbox id="VIdent_identity.get_header_notification" label="&vI_prefDialog.notifyHeaders.label;"
