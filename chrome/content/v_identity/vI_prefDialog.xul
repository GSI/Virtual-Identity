			<checkbox id="VIdent_identity.autoReplyToSelf" label="&vI_prefDialog.autoReplyToSelf.label;"
				prefstring="autoReplyToSelf"/>
			</groupbox>
		</tabpanel>
		<tabpanel orient="vertical">
			<groupbox>
			<caption label="&vI_prefDialog.vIdentityTab.fccTab.caption;"/>
			<hbox align="center">
				<checkbox id="VIdent_identity.doFcc" label="&fccMailFolder.label;" 
					accesskey="&fccMailFolder.accesskey;"
					oncommand="setupFccItems();" prefstring="doFcc"/>
			</hbox>
			<radiogroup id="VIdent_doFcc">
			<grid class="specialFolderPickerGrid">
				<columns>
					<column flex="1"/>
					<column flex="2"/>
				</columns>
				<rows>
					<radio id="VIdent_fcc_Settings_Of_Account" 
						value="3" label="&vI_prefDialog.vidFccAccount.label;"
						oncommand="setPickersState('msgFccFolderPicker', 'msgFccAccountPicker', event)" 
						observes="VIdent_broadcaster_doFcc"/>
					<radio id="VIdent_fcc_Settings_Of_Default" 
						value="2" label="&vI_prefDialog.vidFccDefault.label;"
						oncommand="setPickersState('msgFccFolderPicker', 'msgFccAccountPicker', event)" 
						observes="VIdent_broadcaster_doFcc"/>
					<row align="center">
						<radio id="VIdent_fcc_selectAccount" 
							value="0" label="&sentFolderOn.label;"
							accesskey="&sentFolderOn.accesskey;"
							oncommand="setPickersState('msgFccAccountPicker', 'msgFccFolderPicker', event)" 
							observes="VIdent_broadcaster_doFcc"/>
						<menulist id="msgFccAccountPicker"
							oncommand="noteSelectionChange('VIdent_fcc_selectAccount')" 
							flex="1" observes="VIdent_broadcaster_doFcc"/>
					</row> 
					<row align="center">
						<radio id="VIdent_fcc_selectFolder" 
							value="1" label="&sentInOtherFolder.label;"
							accesskey="&sentInOtherFolder.accesskey;"
							oncommand="setPickersState('msgFccFolderPicker', 'msgFccAccountPicker', event)" 
							observes="VIdent_broadcaster_doFcc"/>
						<menulist id="msgFccFolderPicker" 
							oncommand="noteSelectionChange('VIdent_fcc_selectFolder')" 
							flex="1" observes="VIdent_broadcaster_doFcc"/>
					</row>
				</rows>
			</grid>
			</radiogroup>
			<hbox align="center" class="fccReplyFollowsParent">
				<checkbox wsm_persist="true" id="VIdent_identity.fccReplyFollowsParent"
					  label="&fccReplyFollowsParent.label;"
					  accesskey="&fccReplyFollowsParent.accesskey;"
					  prefattribute="value"
					  prefstring="fccReplyFollowsParent"
					  observes="VIdent_broadcaster_doFcc"/>
			</hbox>

			</groupbox>
		</tabpanel>
		<tabpanel orient="vertical">
			<groupbox>
			<caption />
			<radiogroup id="VIdent_messageDrafts">
			<grid class="specialFolderPickerGrid">
				<columns>
					<column flex="1"/>
					<column flex="2"/>
				</columns>
				<rows>
					<radio id="VIdent_draft_Settings_Of_Account" 
						value="3" label="&vI_prefDialog.vidFccAccount.label;"
						oncommand="setPickersState('msgDraftsFolderPicker', 'msgDraftsAccountPicker', event)" />
					<radio id="VIdent_draft_Settings_Of_Default" 
						value="2" label="&vI_prefDialog.vidFccDefault.label;"
						oncommand="setPickersState('msgDraftsFolderPicker', 'msgDraftsAccountPicker', event)" />
					<row align="center">
						<radio id="VIdent_draft_selectAccount" 
							value="0" label="&draftsFolderOn.label;"
							accesskey="&draftsFolderOn.accesskey;"
							oncommand="setPickersState('msgDraftsAccountPicker', 'msgDraftsFolderPicker', event)" />
						<menulist id="msgDraftsAccountPicker"
							oncommand="noteSelectionChange('VIdent_draft_selectAccount')" />
					</row> 
					<row align="center">
						<radio id="VIdent_draft_selectFolder" 
							value="1" label="&draftInOtherFolder.label;"
							accesskey="&draftInOtherFolder.accesskey;"
							oncommand="setPickersState('msgDraftsFolderPicker', 'msgDraftsAccountPicker', event)" />
						<menulist id="msgDraftsFolderPicker" 
							oncommand="noteSelectionChange('VIdent_draft_selectFolder')" 
							flex="1" />
					</row>
				</rows>
			</grid>
			</radiogroup>

			</groupbox>
		</tabpanel>
		<tabpanel orient="vertical">
			<groupbox>
			<caption />
			<radiogroup id="VIdent_messageTemplates">
			<grid class="specialFolderPickerGrid">
				<columns>
					<column flex="1"/>
					<column flex="2"/>
				</columns>
				<rows>
					<radio id="VIdent_tmpl_Settings_Of_Account" 
						value="3" label="&vI_prefDialog.vidFccAccount.label;"
						oncommand="setPickersState('msgStationeryFolderPicker', 'msgStationeryAccountPicker', event)" />
					<radio id="VIdent_fcc_Settings_Of_Default" 
						value="2" label="&vI_prefDialog.vidFccDefault.label;"
						oncommand="setPickersState('msgStationeryFolderPicker', 'msgStationeryAccountPicker', event)" />
					<row align="center">
						<radio id="VIdent_tmpl_selectAccount" 
							value="0" label="&templatesFolderOn.label;"
							accesskey="&templatesFolderOn.accesskey;"
							oncommand="setPickersState('msgStationeryAccountPicker', 'msgStationeryFolderPicker', event)" />
						<menulist id="msgStationeryAccountPicker"
							oncommand="noteSelectionChange('VIdent_tmpl_selectAccount')" 
							flex="1" />
					</row> 
					<row align="center">
						<radio id="VIdent_tmpl_selectFolder" 
							value="1" label="&templateInOtherFolder.label;"
							accesskey="&templateInOtherFolder.accesskey;"
							oncommand="setPickersState('msgStationeryFolderPicker', 'msgStationeryAccountPicker', event)" />
						<menulist id="msgStationeryFolderPicker" 
							oncommand="noteSelectionChange('VIdent_tmpl_selectFolder')" 
							flex="1" />
					</row>
				</rows>
			</grid>
			</radiogroup>

			</groupbox>
		</tabpanel>
		</tabpanels>
		</tabbox>
	</tabpanel>
	<tabpanel orient="vertical" label="&vI_prefDialog.smartReplyTab.header;">
		<dialogheader title="&vI_prefDialog.smartReplyTab.header;"/>
		<tabbox>
		<tabs>
			<tab id="smartReplyTab1" label="&vI_prefDialog.smartReplyTab.Tab1.label;" />
			<tab id="smartReplyTab2" label="&vI_prefDialog.smartReplyTab.Tab2.label;" />
		</tabs>
		<tabpanels>
			<tabpanel orient="vertical">
			<groupbox>
			<caption label="&vI_prefDialog.smartReplyTab.caption;"/>
			<vbox align="left">
				<description width="500px">&vI_prefDialog.smartReplyTab.Tab1.desc;</description>
				<hbox>
				<vbox><description width="300px">&vI_prefDialog.smartReply.headers.desc;</description>
				<spacer flex="1"/>
				<button id="VIdent_identity.smart_reply_headers_reset" label="&vI_prefDialog.smartReply.headers.reset;" oncommand="vI_prefDialog.base.smartReplyHeaderReset();"/>
				</vbox>
				<textbox id="VIdent_identity.smart_reply_headers" multiline="true"
					rows="4" size="20" wrap="false" flex="1"
					prefstring="smart_reply_headers" />
				</hbox>
			</vbox>
			</groupbox>
			</tabpanel>
			<tabpanel orient="vertical">
			<groupbox>
			<caption label="&vI_prefDialog.smartReplyTab.caption;"/>
			<vbox align="left">
				<description width="500px">&vI_prefDialog.smartReplyTab.Tab2.desc;</description>
				<hbox>
				<vbox><description width="300px">&vI_prefDialog.smartReply.filter.desc;</description><spacer flex="1"/></vbox>
				<textbox id="VIdent_identity.smart_reply_filter" multiline="true"
