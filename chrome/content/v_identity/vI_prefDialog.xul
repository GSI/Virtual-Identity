		<tab label="&vI_prefDialog.notificationTab.label;" />
	</tabs>
	<tabpanels>
	<tabpanel orient="vertical" label="&vI_prefDialog.commonTab.header;">
		<dialogheader title="&vI_prefDialog.commonTab.header;"/>
		<groupbox>
		<caption label="&vI_prefDialog.commonTab.caption;"/>
			<checkbox id="VIdent_identity.storage" label="&vI_prefDialog.storage.label;"
				tooltiptext="&vI_prefDialog.storage.tooltiptext;" prefstring="storage"
				oncommand="vI_prefDialog.base.storageConstraint(this);" />
			<checkbox id="VIdent_identity.autoTimestamp" label="&vI_prefDialog.autoTimestamp.label;"
				tooltiptext="&vI_prefDialog.autoTimestamp.tooltiptext;" prefstring="autoTimestamp" />
			<spacer height="10px"/>
			<label value="&vI_prefDialog.smartInfo.label;" />
			<label id="version-warning"
				value="&vI_prefDialog.versionWarning.label;" hidden="true"/>
			<checkbox id="VIdent_identity.smart_draft" label="&vI_prefDialog.smartDraft.label;"
				tooltiptext="&vI_prefDialog.smartDraft.tooltiptext;" prefstring="smart_draft"
				oncommand="vI_prefDialog.base.idSelectionConstraint();" />
			<checkbox id="VIdent_identity.smart_reply" label="&vI_prefDialog.smartReply.label;"
				tooltiptext="&vI_prefDialog.smartReply.tooltiptext;" prefstring="smart_reply"
				oncommand="vI_prefDialog.base.smartReplyConstraint(this);" />
			<hbox><spacer width="20px"/><checkbox id="VIdent_identity.smart_reply_for_newsgroups" label="&vI_prefDialog.smartReplyNewsgroups.label;"
				tooltiptext="&vI_prefDialog.smartReplyNewsgroups.tooltiptext;" prefstring="smart_reply_for_newsgroups" />
			</hbox>
		</groupbox>
		<groupbox>
		<caption label="&vI_prefDialog.commonTab.caption2;"/>
			<checkbox id="VIdent_identity.show_smtp" label="&vI_prefDialog.SMTP.label;"
				prefstring="show_smtp" />
			<checkbox id="VIdent_identity.menu_entry" label="&vI_prefDialog.menuEntry.label;"
				prefstring="menu_entry" />
			<spacer height="10px"/>
			<checkbox id="VIdent_identity.warn_nonvirtual" label="&vI_prefDialog.warnNonvirtual.label;"
				prefstring="warn_nonvirtual" />
			<checkbox id="VIdent_identity.warn_virtual" label="&vI_prefDialog.warnVirtual.label;"
				prefstring="warn_virtual" />
		</groupbox>
	</tabpanel>
	<tabpanel orient="vertical" label="&vI_prefDialog.vIdentityTab.header;">
		<dialogheader title="&vI_prefDialog.vIdentityTab.header;"/>
		<tabbox>
		<tabs>
			<tab label="&vI_prefDialog.vIdentityTab.valueTab.label;" />
			<tab label="&vI_prefDialog.vIdentityTab.fccTab.label;" />
			<tab label="&vI_prefDialog.vIdentityTab.draftsTab.label;" />
			<tab label="&vI_prefDialog.vIdentityTab.tmplTab.label;" />
		</tabs>
		<tabpanels>
		<tabpanel orient="vertical">
			<groupbox>
			<caption label="&vI_prefDialog.vIdentityTab.valueTab.caption;"/>
			<description width="500px">&vI_prefDialog.vIdentityTab.valueTab.desc;</description>
			<checkbox id="VIdent_identity.copySMIMESettings" label="&vI_prefDialog.SMIME.label;"
				prefstring="copySMIMESettings"/>
			<checkbox id="VIdent_identity.copyEnigmailSettings" label="&vI_prefDialog.PGP.label;"
				prefstring="copyEnigmailSettings"/>
			<checkbox id="VIdent_identity.copyAttachVCardSettings" label="&vI_prefDialog.VCard.label;"
				prefstring="copyAttachVCardSettings"/>
			<spacer height="10px"/>
			<description id="VIdent_identity.HideSignature.warning" width="500px">&vI_prefDialog.HideSignature.warning.desc;</description>
			<checkbox id="VIdent_identity.hide_signature" label="&vI_prefDialog.HideSignature.label;" disabled="true"
				prefstring="hide_signature"/>
